  <Target Name="AfterBuild" Condition=" '$(Configuration)' == 'Release'">
    <Message Text="Run Obfuscator and place result in Obfuscated" />
    <Exec Command="co projectfile=TeaHouse.obproj" />
    <UnZip ZipFileName="$(TargetDir)DiscreteLogics.TeaHouse.vsix" TargetDirectory="$(targetDir)VSIX" />
    <Exec Command="del $(targetDir)VSIX\DiscreteLogics*.dll" />
    <Message Text="copy Obfuscated into vsix, overwriting non-obfuscated files" />
    <Exec Command="xcopy.exe bin\Release\Obfuscated\* bin\Release\VSIX\*  /y" />
    <Message Text="Zip folder back into VSIX" />
    <CreateItem Include="bin\Release\VSIX\**\*">
      <Output ItemName="ZipFiles" TaskParameter="Include" />
    </CreateItem>
    <Exec Command="del $(TargetDir)DiscreteLogics.TeaHouse.unobfuscated.vsix" />
    <Exec Command="rename $(TargetDir)DiscreteLogics.TeaHouse.vsix DiscreteLogics.TeaHouse.unobfuscated.vsix" />
    <Zip ZipFileName="$(TargetDir)DiscreteLogics.TeaHouse.vsix" WorkingDirectory="bin\Release\VSIX" Files="@(ZipFiles)" ZipLevel="0" />
  </Target>
